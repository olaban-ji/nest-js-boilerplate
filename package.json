{
  "name": "nest-js-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --exec 'ts-node -r tsconfig-paths/register'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky",
    "cli": "node dist/main.cli.js",
    "create:user": "npm run cli -- create:user",
    "docs:build": "compodoc -p tsconfig.json -d docs --theme material -n \"nest-js-boilerplate-docs\""
  },
  "mikro-orm": {
    "configPaths": [
      "./src/config/mikro-orm.config.ts"
    ]
  },
